            <TableColumn fx:id="time2" prefWidth="76.0" text="Time" />
            <TableColumn fx:id="emp2" prefWidth="73.0" text="employee" />
            <TableColumn fx:id="cid2" prefWidth="46.0" text="custID" />
            <TableColumn fx:id="cName2s" prefWidth="70.0" text="custName" />
