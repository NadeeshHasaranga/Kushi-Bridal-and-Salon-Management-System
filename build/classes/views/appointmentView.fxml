<AnchorPane id="AnchorPane" fx:id="anchor" prefHeight="497.0" prefWidth="817.0" stylesheets="@newCascadeStyleSheet.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.appointmentViewController">
   <children>
      <TableView fx:id="AppointView" layoutX="30.0" layoutY="100.0" prefHeight="303.0" prefWidth="758.0">
